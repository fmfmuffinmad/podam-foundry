nsive"},"actions":{"value":null},"requirements":{"value":""},"trigger":{"value":""},"deathNote":false,"weapon":{"value":""}},"ownership":{"default":0},"_stats":{"systemId":null,"systemVersion":null,"coreVersion":null,"createdTime":null,"modifiedTime":null,"lastModifiedBy":null}},{"_id":"53Gp7LzK8oOBsDv8","img":"systems/pf2e/icons/actions/OneAction.webp","name":"Blood Drain","sort":500000,"type":"action","effects":[],"folder":null,"flags":{},"system":{"description":{"value":"<p><strong>Requirements</strong> The bloodseeker is attached to a creature.</p>\n<hr />\n<p><strong>Effect</strong> The bloodseeker uses its proboscis to drain blood from the creature it's attached to. This deals [[/r 1d6]]{1d6 damage}, and the bloodseeker gains temporary Hit Points equal to the damage dealt. A creature that has its blood drained by a bloodseeker is @Compendium[pf2e.conditionitems.4D2KBtexWXa6oUMR]{Drained 1} until it receives healing (of any kind or amount).</p>"},"source":{"value":""},"traits":{"value":[],"rarity":"common","custom":""},"rules":[],"slug":null,"schema":{"version":0.793,"lastMigration":{"datetime":"2022-10-08T20:01:24.018-03:00","version":{"schema":0.769,"foundry":"10.286","system":"4.2.1"}}},"actionType":{"value":"action"},"actionCategory":{"value":"offensive"},"actions":{"value":1},"requirements":{"value":""},"trigger":{"value":""},"deathNote":false,"weapon":{"value":""}},"ownership":{"default":0},"_stats":{"systemId":null,"systemVersion":null,"coreVersion":null,"createdTime":null,"modifiedTime":null,"lastModifiedBy":null}},{"_id":"P5bqbR5oTCHvcb36","img":"systems/pf2e/icons/default-icons/lore.svg","name":"Acrobatics","sort":600000,"type":"lore","effects":[],"folder":null,"flags":{},"system":{"description":{"value":""},"source":{"value":""},"traits":{"value":[],"rarity":"common","custom":""},"rules":[],"slug":null,"schema":{"version":0.793,"lastMigration":{"datetime":"2022-10-08T20:01:24.019-03:00","version":{"schema":0.769,"foundry":"10.286","system":"4.2.1"}}}