Collection":"","documentId":null},{"flags":{},"type":0,"text":"A @item[necklace string of small pink pearls]","img":"icons/svg/d20-black.svg","weight":1,"range":[10,10],"drawn":false,"_id":"6j22nlk93540ra8f","documentCollection":"","documentId":null}],"formula":"d10","replacement":true,"displayRoll":true,"folder":"G4JG38FnEPunf7lb","sort":0,"flags":{},"_id":"oQALZ1G6419jCelt","ownership":{"default":0,"74jVYjNGmuLkxIfj":3},"_stats":{"systemId":null,"systemVersion":null,"coreVersion":null,"createdTime":null,"modifiedTime":null,"lastModifiedBy":null}}
